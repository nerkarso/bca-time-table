{
  "name": "bca-time-table",
  "version": "0.11.0",
  "description": "A human readable format of class BCA-1B time table",
  "author": "Ner Karso",
  "keywords": [
    "excel"
  ],
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html --no-cache",
    "build": "parcel build ./src/index.html -d build/ --no-cache --no-source-maps --public-url ./",
    "export": "node ./scripts/export.js",
    "upload:local": "node ./scripts/upload.js",
    "deploy:local": "npm run export && npm run build && npm run upload:local"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "vue": "^2.5.21",
    "vue-icon": "^2.1.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "cssnano": "^4.1.8",
    "node-xlsx": "^0.12.1",
    "parcel": "^1.12.4",
    "sass": "^1.26.10"
  }
}
